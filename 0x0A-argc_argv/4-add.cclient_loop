#include <stdio.h> 
#include <stdlib.h> 
#include <stdbool.h> 

/**
 * is_num - iterate
 * @argvv: aargvv
 * Return: true only if entire string is a number, false if not
 */

bool is_num(char *argvv)
{
	int j = 0;

	for (j = 0; argvv[j]; j++)
	{
		if (!(argvv[j] >= '0' && argvv[j] <= '9'))
			return (0);
	}
	return (1);
}

